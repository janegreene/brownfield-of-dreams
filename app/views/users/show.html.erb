<section class="dashboard-main">
  <h1> <%= current_user.first_name %>'s Dashboard </h1>

  <%= button_to 'Log Out', logout_path, method: 'delete', class: "btn btn-primary mb1 bg-teal" %>
  <%= button_to 'Connect to Github', logout_path, method: 'delete', class: "btn btn-primary mb1 bg-teal" %>
  <h3>Account Details</h3>
  <ul>
    <li> <%= current_user.first_name  %> <%= current_user.last_name %> </li>
    <li> <%= current_user.email%> </li>
  </ul>
  <% if !@results.nil? %>
    <section class="github"><h3>Github</h3>
    <% @repos.each do |resp| %>
      <%= link_to resp.name, resp.html_url %><br/>
    <% end %>
    <section>
      <h3>Followers</h3>
    <% @followers.each do |resp| %>
      <%= link_to resp.login, resp.html_url %><br/>
    <% end %>
      <h3>Following</h3>
    <% @following.each do |resp| %>
      <%= link_to resp.login, resp.html_url %><br/>
    <% end %>
  </section>
    </section>
  <% end %>
  <section>
    <h1>Bookmarked Segments</h1>
  </section>
</section>
